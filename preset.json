{
    "useTaobaoRegistry": true,
    "packageManager": "yarn",
    "useConfigFiles": true,
    "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-router": {
            "historyMode": true
        },
        "@vue/cli-plugin-vuex": {},
        "@vue/cli-plugin-eslint": {
            "config": "prettier",
            "lintOn": [
                "save"
            ]
        },
        "@vue/cli-plugin-unit-jest": {}
    },
    "vueVersion": "3",
    "cssPreprocessor": "node-sass"
}